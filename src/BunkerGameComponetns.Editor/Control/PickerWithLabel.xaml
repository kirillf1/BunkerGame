<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Name="this"
             x:Class="BunkerGameComponetns.Editor.Control.PickerWithLabel">
    <VerticalStackLayout BindingContext="{x:Reference this}" Padding="10" Spacing="10">
        <Label 
            Text="{Binding Title}" />
        <Picker  ItemsSource="{Binding Picker.ItemsSource}"
                 SelectedItem="{Binding Picker.SelectedItem}"></Picker>
    </VerticalStackLayout>
</ContentView>
